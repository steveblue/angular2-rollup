{
  "name": "angular2-rollup",
  "version": "2.1.2",
  "license": "MIT",
  "repository": "https://github.com/steveblue/angular2-rollup",
  "scripts": {
    "clean": "rimraf node_modules ngfactory doc dist && npm cache clean",
    "clean:install": "npm run clean && npm install",
    "clean:dist": "rimraf dist",
    "clean:tmp": "rimraf tmp",
    "clean:ngfactory": "rimraf ngfactory && mkdir ngfactory",
    "bundle:src": "rollup -c rollup.config.js",
    "build:ngc": "ngc -p ./tsconfig.prod.json",
    "transpile:vendor": "tsc --out ./dist/vendor.js --target es5 --allowJs dist/vendor.es2015.js || true",
    "transpile:src": "tsc -p ./tsconfig.dev.json",
    "transpile:babel": "babel ./dist/bundle.es2015.js --out-file ./dist/bundle.js || true",
    "transpile:closure": "java -jar ./compiler.jar --warning_level=QUIET --language_in=ES6 --language_out=ES5 --js ./dist/bundle.es2015.js --js_output_file ./dist/bundle.js",
    "replace:html-dev": "htmlprocessor ./dist/index.html -o ./dist/index.html -e dev",
    "replace:html-prod": "htmlprocessor ./dist/index.html -o ./dist/index.html -e prod",
    "dev:server": "node server.js",
    "webdriver:update": "webdriver-manager update",
    "webdriver:start": "webdriver-manager start",
    "lint": "tslint --force \"src/app/**/*.ts\"",
    "e2e": "protractor protractor.config.js",
    "e2e:live": "protractor protractor.config.js --elementExplorer",
    "pretest": "npm run lint",
    "test": "karma start karma.conf.js",
    "test:watch": "karma start --no-single-run --auto-watch",
    "ci": "npm run e2e && npm run test",
    "start": "rimraf dist && npm run build",
    "build": "node build.js",
    "serve": "node server.js",
    "postinstall": "npm run webdriver:update"
  },
  "dependencies": {
      "@angular/animations" : "4.0.0-rc.6",
      "@angular/common": "4.0.0-rc.6",
      "@angular/compiler": "4.0.0-rc.6",
      "@angular/compiler-cli": "4.0.0-rc.6",
      "@angular/core": "4.0.0-rc.6",
      "@angular/forms": "4.0.0-rc.6",
      "@angular/http": "4.0.0-rc.6",
      "@angular/platform-browser": "4.0.0-rc.6",
      "@angular/platform-browser-dynamic": "4.0.0-rc.6",
      "@angular/platform-server": "4.0.0-rc.6",
      "@angular/router": "4.0.0-rc.6",
      "angular-in-memory-web-api": "~0.2.4",
      "core-js": "^2.4.1",
      "reflect-metadata": "~0.1.8",
      "rxjs": "5.1.1",
      "systemjs": "0.19.47",
      "zone.js": "^0.8.4",
      "express": "^4.14.0",
      "http": "0.0.0",
      "https": "^1.0.0"
    },
    "devDependencies": {
      "@types/core-js": "0.9.36",
      "@types/jasmine": "2.5.46",
      "@types/karma": "0.13.34",
      "@types/node": "7.0.7",
      "autoprefixer": "^6.5.1",
      "babel-cli": "^6.14.0",
      "babel-preset-es2015": "^6.14.0",
      "canonical-path": "0.0.2",
      "chalk": "^1.1.3",
      "chokidar": "^1.6.0",
      "clim": "^1.1.1",
      "codelyzer": "^2.0.1",
      "concurrently": "^3.0.0",
      "core-js": "^2.4.1",
      "cssnano": "^3.8.0",
      "google-closure-compiler": "^20160911.0.0",
      "google-closure-compiler-js": "^20160916.0.0",
      "htmlprocessor": "^0.2.4",
      "http-server": "^0.9.0",
      "jasmine-core": "^2.3.4",
      "jasmine-spec-reporter": "^2.4.0",
      "karma": "1.3.0",
      "karma-chrome-launcher": "^2.0.0",
      "karma-coverage": "^1.0.0",
      "karma-htmlfile-reporter": "^0.3.4",
      "karma-jasmine": "^1.0.2",
      "karma-jasmine-html-reporter": "^0.2.2",
      "karma-mocha-reporter": "^2.0.3",
      "karma-phantomjs-launcher": "^1.0.0",
      "karma-remap-istanbul": "0.2.1",
      "karma-verbose-reporter": "0.0.3",
      "live-server": "^1.1.0",
      "livereload": "^0.6.0",
      "node-sass": "^3.10.1",
      "nodemon": "^1.10.2",
      "npm-run-all": "^3.0.0",
      "onchange": "^3.0.2",
      "phantomjs-prebuilt": "^2.1.13",
      "postcss": "^5.2.5",
      "postcss-cli": "^2.6.0",
      "protractor": "^4.0.10",
      "remap-istanbul": "^0.7.0",
      "replace-in-file": "^2.0.1",
      "rimraf": "^2.5.1",
      "rollup": "^0.41.5",
      "rollup-plugin-alias": "^1.2.0",
      "rollup-plugin-angular": "=0.4.2",
      "rollup-plugin-cleanup": "^1.0.0",
      "rollup-plugin-commonjs": "^7.0.0",
      "rollup-plugin-includepaths": "^0.2.1",
      "rollup-plugin-node-resolve": "^2.0.0",
      "rollup-plugin-replace": "^1.1.1",
      "rollup-plugin-typescript": "^0.8.1",
      "rollup-plugin-uglify": "^1.0.1",
      "shelljs": "^0.7.5",
      "traceur": "0.0.111",
      "ts-helpers": "^1.1.2",
      "tslint": "^4.3.0",
      "typescript": "~2.2.1",
      "watchify": "^3.7.0"
    }

}
